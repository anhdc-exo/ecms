      <Save>Guardar</Save>
      <Cancel>Cancelar</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <searchResultBasePathInputSetAction>
    <title>
      <SelectBasePath>Seleccione la base de ruta</SelectBasePath>
    </title>
  </searchResultBasePathInputSetAction>
  <UISearchForm>
    <message>
      <keyword-not-support>La palabra clave no es válida.  Algunos caracteres no son compatibles.</keyword-not-support>
      <not-checked>Por favor seleccione un tipo de búsqueda.</not-checked>
