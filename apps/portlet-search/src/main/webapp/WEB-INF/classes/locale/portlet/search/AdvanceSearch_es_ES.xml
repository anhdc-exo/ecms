    <label>
      <pageCheckBox>en las páginas</pageCheckBox>
      <documentCheckBox>en el contenido</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>Buscar en todos los sitios</all>
      </option>
    </label>
